#!/bin/bash
#===============================================================
#
# [ author: Krzysztof Baranski ]
#
#       SERVER ( setup )
#
#===============================================================

setuplock='.setuplock'

echo "
---------------------------------------------------
-                  SETUP SERVER                   -
---------------------------------------------------"
#--------------------------------------------------------------| check if configured
if [ -f $setuplock ]
then
	echo "Project alredy configured"
	exit 1
fi

touch $setuplock
chmod 0 $setuplock

echo "---------------------------------------------------
"
